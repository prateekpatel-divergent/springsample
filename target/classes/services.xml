<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="support"
		class="com.divergentsl.initailproject.BeanSupportSample">
		<property name="message" value="Ram Ram"></property>
	</bean>

	<bean name="employee" class="com.divergentsl.thirdsample.Employee">
		<constructor-arg name="id" value="101"></constructor-arg>
		<constructor-arg name="name" value="Amit Yadav"></constructor-arg>
	</bean>
</beans>